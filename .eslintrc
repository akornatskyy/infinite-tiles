{
  "rules": {
    "require-jsdoc": 0,
    "arrow-parens": 0,
    "no-console": 0,
    "linebreak-style": 0,
    "comma-dangle": 0
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": [
    "eslint:recommended",
    "google"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "globals": {
    "describe": true,
    "it": true,
    "expect": true
  }
}
